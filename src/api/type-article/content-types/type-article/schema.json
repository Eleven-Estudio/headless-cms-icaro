{
  "kind": "collectionType",
  "collectionName": "type_articles",
  "info": {
    "singularName": "type-article",
    "pluralName": "type-articles",
    "displayName": "Tipo de art√≠culos",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "name": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string",
      "required": true,
      "unique": true
    },
    "articles": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::call.call",
      "mappedBy": "type"
    },
    "slug": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "uid",
      "targetField": "name"
    },
    "color": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "customField",
      "regex": "^#([A-Fa-f0-9]{6}|[A-Fa-f0-9]{3})$",
      "customField": "plugin::color-picker.color"
    }
  }
}
